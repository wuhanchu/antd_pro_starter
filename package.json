{
    "name": "ant_design_pro_starter",
    "version": "1.1.0",
    "private": true,
    "description": "An out-of-box UI solution for enterprise applications",
    "scripts": {
        "analyze": "cross-env ANALYZE=1 umi build",
        "build": "umi build",
        "build:dataknown": "cross-env BASE_PATH=/z_legal_test umi build",
        "deploy": "npm run site && npm run gh-pages",
        "dev": "npm run start:dev",
        "fetch:blocks": "pro fetch-blocks --branch=umi@3 && npm run prettier",
        "gh-pages": "cp CNAME ./dist/ && gh-pages -d dist",
        "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
        "postinstall": "umi g tmp",
        "lint": "umi g tmp && npm run lint:js && npm run lint:style && npm run lint:prettier",
        "lint-staged": "lint-staged",
        "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
        "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src && npm run lint:style",
        "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
        "lint:prettier": "prettier --check \"**/*\" --end-of-line auto",
        "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
        "prettier": "prettier -c --write \"**/*\"",
        "start": "umi dev",
        "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none umi dev",
        "start:no-mock": "cross-env MOCK=none umi dev",
        "start:no-ui": "cross-env UMI_UI=none umi dev",
        "start:pre": "cross-env REACT_APP_ENV=pre umi dev",
        "start:test": "cross-env REACT_APP_ENV=test MOCK=none umi dev",
        "pretest": "node ./tests/beforeTest",
        "test": "umi test",
        "test:all": "node ./tests/run-tests.js",
        "test:component": "umi test ./src/components",
        "tsc": "tsc"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint-staged"
        }
    },
    "lint-staged": {
        "**/*.less": "stylelint --syntax less",
        "**/*.{js,jsx,tsx,ts,less,md,json}": [
            "prettier --write",
            "git add"
        ]
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 10"
    ],
    "dependencies": {
        "@ant-design/compatible": "1.0.8",
        "@ant-design/icons": "4.3.0",
        "@ant-design/pro-layout": "6.10.9",
        "@ant-design/pro-table": "2.25.1",
        "antd": "4.10.2",
        "axios": "0.21.1",
        "braft-editor": "^2.3.9",
        "classnames": "^2.2.6",
        "client-oauth2": "4.3.3",
        "dva-model-extend": "^0.1.2",
        "file-saver": "2.0.5",
        "lodash": "4.17.20",
        "moment": "2.29.1",
        "nzh": "^1.0.4",
        "omit.js": "2.0.2",
        "paper": "0.12.11",
        "path-to-regexp": "6.2.0",
        "qs": "6.9.6",
        "react": "17.0.1",
        "react-dom": "17.0.1",
        "react-helmet-async": "1.0.7",
        "react-json-view": "^1.20.0",
        "smart-arraytotree": "^1.1.0",
        "styled-components": "^5.2.1",
        "umi": "3.3.5",
        "umi-request": "1.3.5",
        "use-merge-value": "1.0.2",
        "uuid": "8.3.2",
        "xlsx": "0.16.9"
    },
    "devDependencies": {
        "@ant-design/pro-cli": "2.0.4",
        "@types/classnames": "2.2.11",
        "@types/express": "4.17.11",
        "@types/history": "4.7.8",
        "@types/jest": "26.0.20",
        "@types/lodash": "4.14.167",
        "@types/qs": "6.9.5",
        "@types/react": "17.0.0",
        "@types/react-dom": "17.0.0",
        "@types/react-helmet": "6.1.0",
        "@umijs/fabric": "2.4.11",
        "@umijs/plugin-blocks": "2.2.2",
        "@umijs/plugin-qiankun": "^2.14.3",
        "@umijs/preset-ant-design-pro": "1.3.0",
        "@umijs/preset-react": "1.7.12",
        "@umijs/preset-ui": "2.2.9",
        "babel-eslint": "^10.1.0",
        "carlo": "^0.9.46",
        "chalk": "4.1.0",
        "cross-env": "7.0.3",
        "cross-port-killer": "1.3.0",
        "detect-installer": "^1.0.1",
        "enzyme": "^3.11.0",
        "eslint": "7.18.0",
        "eslint-config-prettier": "^7.1.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "3.3.1",
        "express": "^4.17.1",
        "gh-pages": "3.1.0",
        "husky": "4.3.8",
        "jsdom-global": "^3.0.2",
        "lint-staged": "10.5.3",
        "mockjs": "^1.0.1-beta3",
        "prettier": "2.2.1",
        "pro-download": "1.0.1",
        "puppeteer-core": "5.5.0",
        "stylelint": "13.8.0"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "checkFiles": [
        "src/**/*.js*",
        "src/**/*.ts*",
        "src/**/*.less",
        "config/**/*.js*",
        "scripts/**/*.js"
    ]
}
